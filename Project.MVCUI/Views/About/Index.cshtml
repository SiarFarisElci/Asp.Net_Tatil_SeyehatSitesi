<!DOCTYPE HTML>
<html>
<body>

    <div class="content">
        <div class="wrap">
            <div class="about">
                <h3>Hakkımızda</h3>
                @foreach (About item in Model.Abouts)
                {


                <img src="@item.ImageUrl" alt="" />
                <p>@item.Description</p>
                }
            </div>
        </div>
    </div>

</body>
</html>
